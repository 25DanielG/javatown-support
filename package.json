{
    "name": "javatown-support",
    "displayName": "JavaTown Support",
    "description": "Vscode langauge support for my school's java language called JavaTown",
    "version": "0.0.7",
    "publisher": "25DanielG",
    "engines": {
        "vscode": "*"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/25DanielG/javatown-support"
    },
    "keywords": [
        "javatown",
        "support",
        "language",
        "syntax",
        "highlighting",
        "json"
    ],
    "categories": [
        "Programming Languages"
    ],
    "contributes": {
        "languages": [
            {
                "id": "javatown",
                "aliases": [
                    "JavaTown",
                    "javatown"
                ],
                "extensions": [
                    ".jt"
                ],
                "configuration": "./language-configuration.json"
            }
        ],
        "grammars": [
            {
                "language": "javatown",
                "scopeName": "source.jt",
                "path": "./syntaxes/javatown.tmLanguage.json"
            }
        ]
    }
}